        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Data"
                Value="F1 M 15.8333,44.3333C 15.8333,44.3333 14.25,38.7917 26.9167,39.1875C 39.5833,38.7917 38,44.3333 38,44.3333L 15.8333,44.3333 Z M 15.8333,52.25L 38,52.25C 38,52.25 39.5833,57.3958 26.9166,57C 14.25,57.3958 15.8333,52.25 15.8333,52.25 Z M 17.4167,46.3125L 36.4166,46.3125C 37.7283,46.3125 38.7916,46.98 38.7916,48.2917C 38.7916,49.6034 37.7283,50.2709 36.4166,50.2709L 17.4167,50.2709C 16.105,50.2709 15.0417,49.6034 15.0417,48.2917C 15.0417,46.98 16.105,46.3125 17.4167,46.3125 Z M 42.75,57L 38.7916,26.9167L 52.5667,26.9167L 55.4166,17.4167L 58.5833,17.4167L 55.7333,26.9167L 57.7916,26.9167L 53.8333,57L 42.75,57 Z" />
